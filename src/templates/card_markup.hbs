{{#each data}}
  <li class='card'>
    <a href={{largeImageURL}}>
      <img
        class='card__img'
        width='350'
        height='240'
        src={{webformatURL}}
        alt=''
      />
    </a>
    <ul class='card__list'>
      <li class='card__item'>
        <svg width="25" height="25">
          <use href='{{../sprite}}#icon-like'></use>
        </svg>
        <p class='card__value'>{{likes}}</p>
      </li>
      <li class='card__item'>
        <svg width="25" height="25">
          <use href='{{../sprite}}#icon-view'></use>
        </svg>
        <p class='card__value'>{{views}}</p>
      </li>
      <li class='card__item'>
        <svg width="25" height="25">
          <use href='{{../sprite}}#icon-comment'></use>
        </svg>
        <p class='card__value'>{{comments}}</p>
      </li>
      <li class='card__item'>
        <svg width="25" height="25">
          <use href='{{../sprite}}#icon-download'></use>
        </svg>
        <p class='card__value'>{{downloads}}</p>
      </li>

    </ul>
  </li>
{{/each}}
